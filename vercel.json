{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/auth/callback",
      "dest": "/index.html"
    },
    {
      "src": "/auth/callback/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/index.html"
    },
    {
      "src": "/upload",
      "dest": "/index.html"
    },
    {
      "src": "/projects",
      "dest": "/index.html"
    },
    {
      "src": "/projects/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/settings",
      "dest": "/index.html"
    },
    {
      "src": "/billing",
      "dest": "/index.html"
    },
    {
      "src": "/cloud-storage-setup",
      "dest": "/index.html"
    },
    {
      "src": "/pricing",
      "dest": "/index.html"
    },
    {
      "src": "/payment-success",
      "dest": "/index.html"
    },
    {
      "src": "/payment-cancelled",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
